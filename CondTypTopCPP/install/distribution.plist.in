  <options rootVolumeOnly="true"/>
  <choices-outline>
    <line choice="default">
      <line choice="com.typtop.cornell.edu"/>
    </line>
  </choices-outline>
  <choices-outline>
    <line choice="default">
      <line choice="com.typtop.cornell.edu"/>
    </line>
  </choices-outline>
  <choices-outline>
    <line choice="default">
      <line choice="com.typtop.cornell.edu"/>
    </line>
  </choices-outline>
  <choice id="default"/>
