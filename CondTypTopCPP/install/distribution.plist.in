  <background file="../install/typtop-logo.png" alignment="bottomleft" mime-type="image/png" />
  <pkg-ref id="com.typtop.cornell.edu" version="@CPACK_PACKAGE_VERSION@" onConclusion="none">@CPACK_PACKAGE_FILE_NAME@-tmp.pkg</pkg-ref>
